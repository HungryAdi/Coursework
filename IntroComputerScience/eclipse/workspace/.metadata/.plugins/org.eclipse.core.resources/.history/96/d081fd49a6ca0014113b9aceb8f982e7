
public class AdiLinkedList <E>
{

	private Node<E> first = null;
	private int size = 0;
	
	public AdiLinkedList ()
	{
	}
	
	public void addFirst (E e) {
		Node<E> next = new Node<E>(e);
		
	}
	
	
	
	public E removeFirst ( ) {
		return null;
	}
	
	
	public int size() {
		return this.size;
	}
	
	public boolean isEmpty() {
		return (this.size==0);
	}
	
	
	class Node <E> {
		private E element;
		private Node<E> next;
		
		Node(E e) {
			this.element=e;
		}
	}
	
}
